Include config.local

Host *
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%C
  ControlPersist 300
  VisualHostKey yes
  ServerAliveInterval 60
  ServerAliveCountMax 5
  AddKeysToAgent yes
  PubkeyAcceptedKeyTypes=+ssh-rsa
  HostKeyAlgorithms=+ssh-rsa
  #GSSAPIAuthentication yes
  #UseKeychain yes
  #IdentityFile ~/.ssh/id_rsa
  #IdentitiesOnly yes
